{
	"use_algorithm": "SemanticSegmentation",
	"path": "examples/data/SemanticSegmentation",
	"pretrained_weights_path": "examples/data/InstanceSegmentation/logs/pretrained_weights_Lung_SemanticSegmentation.hdf5",
	"batchsize": 1,
	"Iterations_Over_Dataset": 200,
	"data_gen_args": {
			"save_augmented_images": false,
		            "resample_images": false,
		            "std_normalization": false,
		            "feature_scaling": false,
		            "horizontal_flip": false,
		            "vertical_flip": false,
		            "poission_noise": 0,
		            "rotation_range": 45,
		            "zoom_range": 2,
		            "contrast_range": false,
		            "brightness_range": false,
		            "gamma_shift": false,
		            "threshold_background_image": false,
		            "threshold_background_groundtruth": false,
		            "binarize_mask": false
	},
	"loss_function": "binary_crossentropy",
	"num_classes": 1,
	"Image_size": null,
	"calculate_uncertainty": false,
	"evaluation": true
}
